#NOP { /**
 *  @brief  Setzt Trigger fuer Zechpreller Spezialschlaege
 *
 *  @param  1   Erkennungstext fuer den Trigger
 *  @param  2   Endtext fuer Regexp
 *  @param  3   Zeichen
 *  @return     eingefaerbtes Zeichen fuer Effekt
 */};
#FUNCTION {zechspezialschlag} {
    #REGEXP {%1} {%2} {
        #NOP
    }
    {
        #ACTION {%2} {
            #LINE GAG;
            #UNACTION {%2}
        }
    };
    #LINE GAG;
    #RETURN <ffa>%3<099>
}

