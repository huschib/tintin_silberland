#FUNCTION {feind_zielen} {
    #VAR result {};
    #IF { "%0" == "{[A-Za-z\-]+}" } {
        #VAR result {%0}
    };
    #ELSEIF { ("%0" == "{[0-9]+}") && (&{feinde[%0]}) } {
        #VAR result {$feinde[%0]}
    };
    #RETURN {$result}
}

