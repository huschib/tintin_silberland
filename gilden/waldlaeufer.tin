#CLASS waldlaeufer_fertigkeiten OPEN

#ACTION {^ %+ kannst Du %+.} {
    #NOP #ECHO {<cff>Artyrian sagt:<099> %s beherrscht Du %s.} {<ffd>%1<099>} {@barden_fert{%2}<099>};
    #ECHO {%s %s} {@dotfill{<ffd>%1<099>;40}} {@waldlaeufer_fert{%2}<099>};
    #LINE GAG
} {5.0}

#ACTION {^ Im Durchschnitt kannst Du alle Faehigkeiten %+.} {
    #NOP #ECHO {<cff>Artyrian sagt:<099> %s bist Du %s.} {<ffd>Insgesamt betrachtet<099>} {@barden_fert{%1}<099>};
    #ECHO {%s %s} {@dotfill{<ffd>Durchschnitt<099>;40}} {@waldlaeufer_fert{%1}<099>};
    #LINE GAG
} {4.9}

#FUNCTION waldlaeufer_fert {
    #SWITCH { "%0" }     {
        #CASE { "{gut|sehr gut|wirklich toll|fast schon sehr gut|phantastisch gut}" } {
            #VAR result <bca>%0
        };
        #CASE { "{wie ein Waldlaeufer|hervorragend|ausgezeichnet}" } {
            #VAR result <bdb>%0
        };
        #CASE { "{einfach wunderbar|exzellent|meisterhaft}" } {
            #VAR result <dea>%0
        };
        #CASE { "{unglaublich gut|virtuos|nahezu perfekt|perfekt}" } {
            #VAR result <bfa>%0
        };
        #CASE { "{unuebertreffbar|absolut perfekt}" } {
            #VAR result <dfc><388>%0
        };
        #DEFAULT {
            #VAR result <bba>%0
        }
    };
    #RETURN $result
}

