#NOP { /**
 *  @brief  Zeigt Stufen fuer den Skill-Fortschritt
 *
 */};
#ALIAS {wfort} {
    #VAR tmp_count {1};
    #SCREEN GET {SCROLL_COLS} {tmp_width};
    #VAR tmp_line {};
    #WHILE {$tmp_count <= &wfort[] } {
        #FORMAT {tmp_line_len} {%L} {$tmp_line};
        #FORMAT {tmp_item_len} {%L} {$wfort[$tmp_count]};
        #MATH {tmp_total_len} {$tmp_line_len + $tmp_item_len + 5};
        #IF {$tmp_count < 15} {
            #VAR tmp_color {<bba>}
        };
        #ELSEIF {$tmp_count < 19} {
            #VAR tmp_color {<bca>}
        };
        #ELSEIF {$tmp_count < 22} {
            #VAR tmp_color {<bdb>}
        };
        #ELSEIF {$tmp_count < 24} {
            #VAR tmp_color {<dea>}
        };
        #ELSEIF {$tmp_count < 26} {
            #VAR tmp_color {<bfa>}
        };
        #ELSE {
            #VAR tmp_color {<dfc><388>}
        };
        #IF {$tmp_total_len > $tmp_width} {
            #ECHO {$tmp_line<099>};
            #VAR tmp_line {$tmp_color$wfort[$tmp_count]};
        };
        #ELSEIF {"$tmp_line" == ""} {
            #VAR tmp_line {$tmp_color$wfort[$tmp_count]};
        };
        #ELSE {
            #CAT tmp_line {     $tmp_color$wfort[$tmp_count]}
        };
        inc tmp_count
    };
    #ECHO {$tmp_line<099>};
} {5};

